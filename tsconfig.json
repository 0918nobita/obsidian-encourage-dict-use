{
  "compilerOptions": {
      "target": "es2018",
      "module": "esnext",
      "moduleResolution": "bundler",
      "baseUrl": ".",
      "noEmit": true,
      "esModuleInterop": true,
      "isolatedModules": true,
      "forceConsistentCasingInFileNames": true,
      "strict": true,
      "skipLibCheck": true
  }
}
